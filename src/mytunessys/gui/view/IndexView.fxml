<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="brdrPane" prefHeight="571.0" prefWidth="373.0" styleClass="borderPane" stylesheets="@../css/temp.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mytunessys.gui.controller.BaseController">
   <center>
      <AnchorPane fx:id="contentWindow" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane fx:id="top" prefHeight="60.0" prefWidth="374.0">
               <children>
                  <Label fx:id="lblCurrentLocation" layoutX="48.0" layoutY="15.0" text="Label" styleClass="currentSiteLabel" />
                  <Button fx:id="btnGoBack" focusTraversable="false" layoutX="7.0" layoutY="17.0" mnemonicParsing="false" text="&lt;" />
                  <Button fx:id="btnAdd" focusTraversable="false" layoutX="335.0" layoutY="17.0" mnemonicParsing="false" onAction="#NewItem" text="+" />
                  <TextField fx:id="txfSearchBar" focusTraversable="false" layoutX="179.0" layoutY="18.0" promptText="Search..." />
               </children>
            </AnchorPane>
            <AnchorPane fx:id="centerContent" layoutX="14.0" layoutY="60.0" prefHeight="401.0" prefWidth="346.0" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <VBox prefHeight="98.0" prefWidth="374.0" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane styleClass="bottomPanel" minHeight="0.0" minWidth="0.0" prefHeight="51.0" prefWidth="374.0">
               <children>
                  <HBox prefHeight="45.0" prefWidth="373.0">
                     <children>
                        <VBox prefHeight="45.0" prefWidth="209.0">
                           <children>
                              <Label fx:id="lblNameOfSong" styleClass="songLabel" prefHeight="17.0" prefWidth="182.0" text="Name of Song" />
                              <Label fx:id="lblArtist" styleClass="songArtist" prefHeight="17.0" prefWidth="309.0" text="Artist" />
                           </children>
                        </VBox>
                        <VBox prefHeight="45.0" prefWidth="50.0">
                           <children>
                              <Button fx:id="btnPrevious" focusTraversable="false" minHeight="40" minWidth="45" mnemonicParsing="false" styleClass="btnPrev" />
                           </children>
                        </VBox>
                        <VBox prefHeight="45.0" prefWidth="50.0">
                           <children>
                              <Button fx:id="btnPlay" focusTraversable="false" minHeight="50" minWidth="50" mnemonicParsing="false" styleClass="btnPlay" />
                           </children>
                        </VBox>
                        <VBox layoutX="240.0" layoutY="10.0" prefHeight="45.0" prefWidth="50.0">
                           <children>
                              <Button fx:id="btnNext" focusTraversable="false" minHeight="40" minWidth="45" mnemonicParsing="false" styleClass="btnNext" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <AnchorPane styleClass="bottomPanel" minHeight="0.0" minWidth="0.0" prefHeight="45.0" prefWidth="374.0">
               <children>
                  <HBox prefHeight="43.0" prefWidth="373.0">
                     <children>
                        <VBox prefHeight="43.0" prefWidth="185.0">
                           <children>
                              <Button fx:id="btnSongs" focusTraversable="false" maxWidth="30" minWidth="20" mnemonicParsing="false" onAction="#switchToSongInterface" />
                              <Label text="Songs" />
                           </children>
                           <padding>
                              <Insets left="65.0" />
                           </padding>
                        </VBox>
                        <VBox prefHeight="43.0" prefWidth="185.0">
                           <children>
                              <Button fx:id="btnPlaylists" focusTraversable="false" mnemonicParsing="false" onAction="#switchToPlaylistInterface" />
                              <Label text="Playlists" />
                           </children>
                           <padding>
                              <Insets left="65.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </bottom>
</BorderPane>
